{
    "workbench.sideBar.location": "right",
    "workbench.colorCustomizations": {
      "terminal.foreground" : "#6f922e"
    },
    "saveAndRun": {
        "commands": [
            {
              "match": ".C",
              "cmd": "gcc -o cpractice cpractice.c -lm",
              "useShortcut": false,
              "silent": false
            },
            {
              "match": "\\.c$",
              "cmd": "./cpractice",
              "useShortcut": false,
              "silent": false
            },
            {
              "match": ".asm",
              "cmd": "fasm x86practice.asm",
              "useShortcut": false,
              "silent": false
            },
            {
              "match": "\\.asm$",
              "cmd": "./x86practice",
              "useShortcut": false,
              "silent": false
            },
            {
              "match": ".cpp",
              "cmd": "g++ cpppractice.cpp -std=c++20",
              "useShortcut": false,
              "silent": false
            },
            {
              "match": "\\.cpp$",
              "cmd": "./a.exe",
              "useShortcut": false,
              "silent": false
            },
            {
              "match": "practice.py",
              "cmd": " & c:/Users/ravir/OneDrive/Documents/Python/pandasenv/Scripts/python.exe c:/Users/ravir/OneDrive/Documents/Python/pythonpractice/practice.py",
              "useShortcut": false,
              "silent": false
            }
          ]
    },
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.tabs.enabled": false,
    "editor.fontSize": 16,
    "[bibtex]": {}
}

